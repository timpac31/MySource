<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="MyTest">

	<typeAlias alias="myResult" type="xml.dao.mytest.IbatisBean"/> 
	
   <select id="getAll" parameterClass="myResult" resultClass="myResult">
      SELECT seq,name,center,regdate, applynum, remark FROM GSEDU
      WHERE 1=1 
      <isNotEmpty property="seq" prepend="AND">
      seq = #seq#
      </isNotEmpty>
   </select>
   
   <select id="getDetail" parameterClass="java.lang.Integer" resultClass="myResult">
   		SELECT seq,name,center,regdate, applynum, remark FROM GSEDU WHERE SEQ = #seq#
   </select>
	
</sqlMap>